#!/bin/sh

. ./common.conf

case $1 in
	--i)
		sudo gpg --import $keys_dir/*.new
	;;
	--e) 
		sudo gpg --export $kayname > $2/$kname
	;;
	--f)
		sudo gpg --export $kayname2 > $2/KsK
	;;
	--m)
		echo "cd $keys_dir"
		echo "gpg --generate-key"
		echo "gpg --generate-key"
	;;
	*)
		echo "$0 --i OR "
		echo "$0 --e <dir> "	
	break
	;;
esac
