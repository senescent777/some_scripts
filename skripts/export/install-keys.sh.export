#!/bin/sh
. ./common.conf
. ./common_funcs.sh

usage() {
	echo "$0 --i Imports keys"
	echo "$0 --m generates 2 keys"
}
parse_opts $1 $2

case $cmd in
	--i)
		sudo gpg --import $keys_dir/*.new
	;;
	--e) 
		sudo gpg --export $CONF_kayname > $par/$TARGET_Dkname1
	;;
	--f)
		sudo gpg --export $CONF_kayname2 > $par/TARGET_Dkname2
	;;
	--m)
		echo "cd $keys_dir"
		echo "gpg --generate-key"
		echo "gpg --generate-key"
	;;
	*)
		usage
	break
	;;
esac
